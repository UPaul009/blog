{
  "project": "blog",
  "version": 2,
  "alias": [
    "www.mcansh.blog",
    "mcansh.blog"
  ],
  "regions": [
    "all"
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next@canary",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/changelog",
      "status": 301,
      "headers": {
        "Location": "https://github.com/mcansh/blog/releases"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "/static/images/logo/logo.ico",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/manifest.webmanifest",
      "dest": "/static/manifest.webmanifest",
      "headers": {
        "content-type": "application/manifest+json",
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/static/manifest.webmanifest",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/feed.json",
      "dest": "/static/feed.json",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/sitemap.xml",
      "dest": "/static/sitemap.xml",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/atom",
      "dest": "/static/atom.xml",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/sw.js",
      "dest": "/_next/static/sw.js",
      "headers": {
        "cache-control": "max-age=0",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/google2b07cf64e0755399.html",
      "dest": "/static/google2b07cf64e0755399.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "TWITTER": "loganmcansh",
    "INSTAGRAM": "loganmcansh",
    "GITHUB": "mcansh",
    "EMAIL": "logan@mcan.sh",
    "SENTRY": "https://07a54d3b59bb4bf5ad1c6ddf050d51c1@sentry.io/197817",
    "ANALYTICS": "UA-87731356-4"
  }
}
