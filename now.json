{
  "project": "blog",
  "version": 2,
  "alias": [
    "www.mcansh.blog",
    "mcansh.blog"
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next@canary",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/static/manifest.json"
    },
    {
      "src": "/feed.json",
      "dest": "/static/feed.json"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/static/sitemap.xml"
    },
    {
      "src": "/atom",
      "dest": "/static/atom.xml"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "TWITTER": "@blog_twitter",
    "INSTAGRAM": "@blog_instagram",
    "GITHUB": "@blog_github",
    "EMAIL": "@blog_email",
    "SENTRY": "@blog_sentry",
    "ANALYTICS": "@blog_analytics",
    "GITHUB_TOKEN": "@blog_github_token"
  }
}
